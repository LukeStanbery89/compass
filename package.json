{
    "name": "@lukestanbery/compass",
    "version": "1.0.3",
    "description": "Package for interfacing with a directory of ports and static IPs addresses on the LAN.",
    "source": "src/index.js",
    "main": "dist/index.js",
    "targets": {
        "main": {
            "optimize": true
        }
    },
    "scripts": {
        "build": "parcel build",
        "postinstall": "if [ -e ./scripts/postinstall.sh ]; then ./scripts/postinstall.sh; fi",
        "prepublishOnly": "npm run build",
        "test-unit": "jest --coverage=true ./test/unit --verbose --runInBand"
    },
    "files": [
        "dist/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LukeStanbery89/compass.git"
    },
    "keywords": [
        "ip",
        "port"
    ],
    "author": "Luke Stanbery",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LukeStanbery89/compass/issues"
    },
    "homepage": "https://github.com/LukeStanbery89/compass#readme",
    "devDependencies": {
        "@jest/types": "^29.0.1",
        "jest": "^29.0.1",
        "node-notifier": "^10.0.1",
        "parcel": "^2.7.0"
    },
    "dependencies": {
        "@lukestanbery/satchel": "^1.0.4"
    }
}